{
  "manifest_version": 3,
  "name": "Manjaro Wiki Search",
  "short_name": "manjaro_search_wiki",
  "description": "Adds Manjaro Wiki as a search engine.",
  "version": "1.0",
  "icons": {
    "16": "icons/manjaro.svg",
    "64": "icons/manjaro.svg",
    "256": "icons/manjaro.svg"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Manjaro Wiki",
      "keyword": "@manw",
      "favicon_url": "icons/manjaro.svg",
      "search_url": "https://wiki.manjaro.org/index.php?search={searchTerms}"
    }
  },
  "browser_specific_settings": {
    "gecko": {
        "id": "ManWiki@manjaro.org",
        "strict_min_version": "109.0"
    }
  },
  "author": "Mirdarthos",
  "homepage_url": "https://github.com/Mirdarthos/firefox-extension-manjaro-search"
}
